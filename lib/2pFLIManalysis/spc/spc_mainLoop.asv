function out=spc_mainLoop;

	global state gh
	
	setStatusString('Looping...');
    set(gh.spc.FLIMimage.grab, 'Visible', 'off');
    set(gh.spc.FLIMimage.focus, 'Visible, 'off');
    state.spc.acq.timer.looptimer =timer('TimerFcn','spc_LoopFcn','ExecutionMode','fixedSpacing','Period',state.standardMode.repeatPeriod);
	
	